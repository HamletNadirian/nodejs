{
  "name": "nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node build/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed": "tsx src/seed/seedRunner.ts",
    "seed:dev": "npm run build && node build/seed/seedRunner.js",
    "start:with-seed-test": "npm run build && npm test && npm run seed && npm start",
    "dev:with-seed": "npm run seed && npm run dev",
    "reset-db": "npm run seed:dev",
    "start-and-seed": "node build/index.js & tsx src/seed/seedRunner.ts"
  },
  "dependencies": {
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "concurrently": "^9.1.0",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "http-status-codes": "2.3.0",
    "log4js": "6.9.1",
    "mongodb-memory-server": "^11.0.1",
    "mongoose": "^8.21.0",
    "openapi-types": "^12.1.3",
    "reflect-metadata": "0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.20.0",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
