    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "eslint": "9.39.2",
    "eslint-plugin-jest": "28.11.0",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "supertest": "7.0.0",
    "testcontainers": "10.28.0",
    "ts-jest": "29.2.5",
    "tsx": "4.21.0",
    "typescript": "5.7.2",
    "typescript-eslint": "8.24.1"
  },
